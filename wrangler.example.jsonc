{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "r2-image-worker",
  "workers_dev": true,
  "compatibility_date": "2025-08-09",
  "r2_buckets": [
    {
      "binding": "BUCKET",
      "bucket_name": "images",
      "preview_bucket_name": "images"
    }
  ],
  "images": {
    "binding": "IMAGES",
  }
}